  "scripts": {
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "npm run lint:check --fix",
    "lint:check": "eslint \"{src,database,apps,libs,tests}/**/*.ts\" --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:de": "npm run docker:up && docker compose exec api npx prisma migrate deploy && jest -i",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
